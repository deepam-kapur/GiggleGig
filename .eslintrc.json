{
    "extends": "airbnb-base",
    "rules": {
      "max-len": [
        "error",
        {
          "code": 200,
          "tabWidth": 2,
          "ignoreUrls": true,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true
        }
      ],
      "no-underscore-dangle": "off"
    }
}