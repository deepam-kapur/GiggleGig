{

  "extends": "airbnb-base",
  "rules": {
      "max-len": [
        "error",
        {
          "code": 200,
          "tabWidth": 2,
          "ignoreUrls": true,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true
        }
      ],      
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "always",
        "json": "always"
      }
    ],
    "no-unused-vars": "off"
  }
}